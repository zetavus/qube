// 양자 물리학 기호 테스트 - 수정됨

fn main() {
    println("🔬 양자 물리학 기호 테스트:");
    
    // 먼저 π 정의
    pi@ = 3.14159;
    
    // 파동함수와 확률
    psi@ = 0.707;  // |ψ⟩ 상태
    phi@ = 0.707;  // |φ⟩ 상태
    
    println("파동함수 ψ = {}", psi@);
    println("파동함수 φ = {}", phi@);
    
    // 각도와 위상
    theta@ = pi@ / 4;  // θ = π/4
    alpha@ = pi@ / 6;  // α = π/6
    
    println("각도 θ = {} (= π/4)", theta@);
    println("위상 α = {} (= π/6)", alpha@);
    
    // 수학 연산자들
    println("∑ (sum@) 연산자 사용 가능");
    println("∏ (prod@) 연산자 사용 가능");
    println("∫ (integral@) 연산자 사용 가능");
    
    // 집합과 논리
    println("∀ (forall@) 전칭한정자");
    println("∃ (exists@) 존재한정자");
    println("∈ (in@) 원소관계");
    println("ℂ (complex@) 복소수 집합");
    
    println("✅ 모든 수학 기호 변환 성공!");
}