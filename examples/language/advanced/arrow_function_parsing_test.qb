fn main() {
    println("=== 화살표 함수 파싱 테스트 ===");
    
    // 1단계: 기본 파싱 테스트
    println("1단계: 기본 파싱 준비");
    
    // 2단계: 단일 매개변수 화살표 함수 (주석 해제!)
    scalar doubler = x => x * 2;
    println("2단계: 단일 매개변수 화살표 함수 파싱 성공");
    
    // 3단계: 다중 매개변수 화살표 함수 (아직 주석)  
    scalar adder = (a, b) => a + b;
    println("3단계: 다중 매개변수 화살표 함수 파싱 성공");
    
    // 4단계: 복잡한 표현식 (아직 주석)
    scalar complex = x => x * 2 + 1;
    println("4단계: 복잡한 표현식 파싱 성공");
    
    println("파싱 테스트 1단계 완료!");
}