class Person {
    private name: str;
    private age: i32;
    
    constructor() {
        self.name = "Unknown";
        self.age = 0;
        println("âœ… Default constructor: {}", self.name);
    }
    
    constructor(name: str) {
        self.name = name;
        self.age = 0;
        println("âœ… Name constructor: {}", self.name);
    }
    
    constructor(name: str, age: i32) {
        self.name = name;
        self.age = age;
        println("âœ… Full constructor: {} ({})", self.name, self.age);
    }
    
    destructor() {
        println("ğŸ—‘ï¸ Destructor: Goodbye {}", self.name);
    }
    
    public fn get_name(self) -> str {
        return self.name;
    }
}

fn main() {
    println("ğŸ§ª Constructor/Destructor execution test");
    
    // QyTum ë°©ì‹ìœ¼ë¡œ ë³€ìˆ˜ ì„ ì–¸
    const p1 = Person();
    const p2 = Person("Alice");  
    const p3 = Person("Bob", 25);
    
    println("Name: {}", p1.get_name());
    println("Name: {}", p2.get_name());
    println("Name: {}", p3.get_name());
    
    println("Test completed!");
}