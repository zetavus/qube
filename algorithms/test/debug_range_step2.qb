// algorithms/test/debug_range_step2.qb
// ~ 토큰 대신 다른 방법으로 테스트

fn test_without_tilde() {
    println("🔧 ~ 없이 기본 테스트");
    println("====================");
    
    x = 5;
    y = x + 1;  // ~ 대신 일반 연산
    
    println("일반 연산 테스트: {} + 1 = {}", x, y);
}

fn main() {
    println("🎯 ~ 토큰 없는 기본 동작 확인");
    println("===========================");
    
    test_without_tilde();
    
    println("✅ 기본 표현식 파싱 정상");
}